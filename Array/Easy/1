class Solution {
public:
    vector<int> twoSum(vector<int>& v, int target) {
         int x,y; 
        for(int i=0;i<v.size()-1;i++)
        {
            for(int j=i+1;j<v.size();j++)
            {
                if(v[i]+v[j]==target)
                {
                    x=i,y=j;
                 }
             }
        }
        return {x,y};
        
    }
};
